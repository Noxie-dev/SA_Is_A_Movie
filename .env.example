# Auth0 Configuration
# Copy this file to .env and replace these values with your actual Auth0 credentials

VITE_AUTH0_DOMAIN=your-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your-client-id-here

# Contentful Configuration
# Get these values from your Contentful space settings

VITE_CONTENTFUL_SPACE_ID=your-space-id-here
VITE_CONTENTFUL_ACCESS_TOKEN=your-access-token-here
